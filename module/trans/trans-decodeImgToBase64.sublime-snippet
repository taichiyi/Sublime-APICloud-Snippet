    <content><![CDATA[
trans.decodeImgToBase64({
    imgPath: '${1:fs://1.png}'
}, function(ret, err){
    if (ret.status) {
        api.alert({msg:ret.base64Str});
    } else{
        api.alert({msg:err.msg});
    }
});
]]></content>
